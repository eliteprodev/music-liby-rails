<h1>Add a new song:</h1>
<%= form_for(@song) do |f| %>

<%= f.label :title %>
<%= f.text_field :title %>
<%= f.label :release_date %>
<%= f.text_field :release_date %>
<%= f.label :album %>
<%= f.text_field :album %>
<%= f.label :songfile %>
<%= f.file_field :songfile %>
<br>
<br>
<br>
<%= f.label :artist_id %>
<%= f.collection_select :artist_id, Artist.all, :id, :name, :prompt => true %> 
<br>
<br>
<br>
<%= f.submit "Edit Song", :class => "btn btn-primary btn-lg green darken-1"%>
<% end %>
