<tr>
    <td><%= link_to song.title, song, style: "font-size: 15px;" %></td>
    <td><%= link_to song_path(song) do %><%= image_tag song.songfile, style: "width:75px;" %><% end %></td>
    <td><%= link_to song.artist.name, artist_path(song.artist) %></td>
    <td><%= song.release_date %></td>
</tr>
