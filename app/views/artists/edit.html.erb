<h1>Edit the artist:</h1>
<%= form_for(@artist) do |f| %>

<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :birthday %>
<%= f.text_field :birthday %>
<%= f.label :artistpicture %>
<%= f.file_field :artistpicture %>
<br>
<br>
<br>
<%= f.submit "Edit Artist", :class => "btn btn-primary btn-lg green darken-1"%>
<% end %>
